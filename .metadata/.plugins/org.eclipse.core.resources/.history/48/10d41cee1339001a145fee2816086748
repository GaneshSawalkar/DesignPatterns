package database;

public class student {
	int id;
	String first;
	int age;
	private static student s;

	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}

	/**
	 * @return the first
	 */
	public String getFirst() {
		return first;
	}

	/**
	 * @return the age
	 */
	public int getAge() {
		return age;
	}

	static student getinstance() {
		if (s == null) {
			s = new student();
		}
		return s;
	}

}
