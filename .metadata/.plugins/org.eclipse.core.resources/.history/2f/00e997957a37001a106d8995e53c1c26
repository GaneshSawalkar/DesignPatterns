
public class mytopicsubscriber implements Observe {
	private String name;
	private Subject topic;

	public mytopicsubscriber(String nm) {
		this.name = nm;
	}

	public void update() {
		String msg = (String) topic.getupdate((Observe) this);
		if (msg == null) {
			System.out.println(name + ":: No new message");
		} else
			System.out.println(name + ":: Consuming message::" + msg);
	}

	public void setSubject(Subject sub) {
		this.topic = sub;
	}
}
